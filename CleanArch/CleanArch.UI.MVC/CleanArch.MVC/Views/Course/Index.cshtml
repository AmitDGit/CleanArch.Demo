@model  IEnumerable<CleanArch.Application.ViewModels.CoursesViewModel>

<div>
    <div>
        @foreach (var course in Model)
        {
            <div>
                <div style="background-image:url(@course.ImageUrl)"></div>
            </div>
            <span>@course.Name</span>
        }
    </div>
</div>